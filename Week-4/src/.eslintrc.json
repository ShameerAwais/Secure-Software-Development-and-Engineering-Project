{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "import/prefer-default-export": "off",
    "no-param-reassign": ["error", { "props": false }],
    "max-len": ["error", { "code": 100 }],
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  }
} 